{
	"compilerOptions": {
		"outDir": "./public/",
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "es5",
		"jsx": "react"
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules"],
	"awesomeTypescriptLoaderOptions": {
		"useBabel": true,
		"babelOptions": {
			"babelrc": false,
			"presets": [["@babel/preset-env", { "targets": "last 5 versions, ie 11", "modules": false }]]
		},
		"babelCore": "@babel/core"
	}
}
