{
	"compilerOptions": {
		"jsx": "react",
		"module": "commonjs",
		"noImplicitAny": true,
		"outDir": "dist",
		"skipLibCheck": true,
		"sourceMap": true,
		"target": "es5"
	},
	"include": ["./src/**/*"],
	"awesomeTypescriptLoaderOptions": {
		"useBabel": true,
		"babelOptions": {
			"babelrc": false,
			"presets": [["@babel/preset-env", { "targets": "last 5 versions, ie 11", "modules": false }]]
		},
		"babelCore": "@babel/core"
	}
}
